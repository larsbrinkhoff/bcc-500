SAKK   SKR     ALPCNT
       BRU     MORE
       BRR*    SKALP
         
THREAD ZRO     THRD
       LDA     NUMB
       SKG     MONE
       BRU     IMMED
       LDX     FLINE
       SKG     0,2
       BRU     NEWFLN
       STA*    LNHDR
       EAX     3,2
LNOVER SKG     -3,2            7 CYCLE SEARCH
       BRU     LNCHK
       CXB
       LDX     -2,2
       BRU     LNOVER
LNCHK  SKE     -3,2
       BRU     LNLESS
       LDA     0,2
       ETR     =77740000B      TBG 6/16/68
       SKE     CDE10
       BRU     LNCON
       LDA     MONE             REPLACE DATA STATEMENT
       STA     2,2
LNCON  LDA     -2,2
       STA*    WTING
LNOUT  LDA     WTING
       ADD     TWO
       CBX
       STA     -2,2
       BRU     IMAGE
LNLESS STX*    WTING
       BRU     LNOUT
NEWFLN SKE     0,2
       BRU     NEWBG
       LDA     1,2
VERFST STA*    WTING
NEWOUT LDA     LNHDR
       STA     FLINE
       LDA     NUMB
       STA*    LNHDR
       BRU     IMAGE
NEWBG  LDA     0,2
       SKE     LINE
       BRU     NEWFST
       LDA     EOFL
       BRU     VERFST
NEWFST EAX     3,2
       STX*    WTING
       BRU     NEWOUT
