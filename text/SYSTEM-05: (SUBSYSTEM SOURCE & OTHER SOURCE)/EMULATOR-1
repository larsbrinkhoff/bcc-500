        PROGRAM MISC'POPS;
        INCLUDE POPVARS;
        SET'LE;
        JUMP'LE;
        ENTRY SBRR'(),DOSYSPOP(71B);
        SAVEDSR‚ÜêSAVEDSR V' 2 IF ((SPEA.ADDR14.W0) A' 4B7) # 0;
        POPRET;

         ENTRY SBRM'(), DOSYSPOP(70B);
        IF (SAVEDSR A' 2) # 0 DO;
            (SPEA.ADDR14.W0)‚ÜêSAVEDP V' 432B5;
        ELSE DO;
            (SPEA.ADDR14.W0)‚ÜêSAVEDP V' 032B5;
        ENDIF;
        SAVEDP‚ÜêSPEA; POPRET;

         ENTRY STP'(),DOSYSPOP(67B);
        $SPEA‚ÜêSAVEDAB; POPRET;

         ENTRY LDP'(),DOSYSPOP(66B);
        SAVEDAB‚Üê$SPEA; POPRET;

         ENTRY GCI'(),DOSYSPOP(65B);
        IF SPEA.W0>=SPEA.W1 DO;
            POPRET;
        ENDIF;
        SPEA.W0‚ÜêSPEA.W0+1;
        MAKE'STRING'IAW(SPEA.W0,I);
        SAVEDA‚Üê$I; POPSKIPRET;

         ENTRY WCH'(),DOSYSPOP(64B);
        IF SPEA.W0<SPEA.W1 DO;
            SPEA.W0‚ÜêSPEA.W0+1; MAKE'STRING'IAW(SPEA.W0,I); $I‚ÜêSAVEDA;
            POPRET;
        ELSE DO;
            SAVEDB‚ÜêSAVEDP; SAVEDP‚Üê(SPEA+2)$ADDR14-1; POPRET;
        ENDIF;


         ENTRY WCI'(),DOSYSPOP(57B);
        SPEA.W1‚ÜêSPEA.W1+1;
        MAKE'STRING'IAW(SPEA.W1,I); $I‚ÜêSAVEDA; POPRET;

         ENTRY GCD'(),DOSYSPOP(37B);
        IF SPEA.W0>=SPEA.W1 DO;
            POPRET;
        ENDIF;
        MAKE'STRING'IAW(SPEA.W1,I); SAVEDA‚Üê$I;
        SPEA.W1‚ÜêSPEA.W1-1; POPSKIPRET;

         ENTRY WCD'(),DOSYSPOP(35B);
        MAKE'STRING'IAW(SPEA.W0,I); $I‚ÜêSAVEDA;
        SPEA.W0‚ÜêSPEA.W0-1; POPRET;

         ENTRY BKPT'(),DOSYSPOP(33B);
        BRS10();* EXITS TO RUN'NEXT'FORK
        END;

